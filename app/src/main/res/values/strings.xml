    <string name="settings">Settings</string>

    <string name="key_activeIndicatorFillColor">activeIndicatorColorFill</string>
    <string name="title_activeIndicatorFillColor">Active indicator fill color</string>
    <color name="default_value_activeIndicatorFillColor">@color/activeIndicatorColorFill</color>

    <string name="key_activeIndicatorStrokeColor">activeIndicatorColorStroke</string>
    <string name="title_activeIndicatorStrokeColor">Active indictor stroke color</string>
    <color name="default_value_activeIndicatorStrokeColor">@color/activeIndicatorColorStroke</color>

    <string name="key_activeIndicatorFillSize">activeIndicatorSize</string>
    <string name="title_activeIndicatorFillSize">Active indicator size</string>
    <dimen name="default_value_activeIndicatorFillSize">10dp</dimen>
    <string name="summary_activeIndicatorSize">%1$sdp</string>

    <string name="key_activeIndicatorStrokeWidth">activeIndicatorStrokeWidth</string>
    <string name="title_activeIndicatorStrokeWidth">Active indicator stroke width</string>
    <dimen name="default_value_activeIndicatorStrokeWidth">1dp</dimen>
    <string name="summary_activeIndicatorStrokeWidth">%1$sdp</string>


    <string name="key_inactiveIndicatorFillColor">inactiveIndicatorColorFill</string>
    <string name="title_inactiveIndicatorFillColor">Inactive indicator fill color</string>
    <color name="default_value_inactiveIndicatorFillColor">@color/inactiveIndicatorColorFill</color>

    <string name="key_inactiveIndicatorStrokeColor">inactiveIndicatorColorStroke</string>
    <string name="title_inactiveIndicatorStrokeColor">Inactive indicator stroke color</string>
    <color name="default_value_inactiveIndicatorStrokeColor">@color/inactiveIndicatorColorStroke
    </color>

